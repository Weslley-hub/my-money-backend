{
  "name": "my-money-backend",
  "version": "1.0.0",
  "description": "Projeto criado na disciplina de projeto integrador II com o foco de criar uma sistema de gerenciamento de gastos",
  "main": "./src/main.ts",
  "scripts": {
    "dev": "NODE_ENV=dev ts-node-dev --ignore-watch ./node_modules --respawn --transpile-only ./src/server.ts",
    "knex:migration:run": "knex --knexfile ./src/config/knexfile.ts migrate:latest",
    "knex:migration:rollback": "knex --knexfile ./src/config/knexfile.ts migrate:rollback",
    "knex:migration:make": "knex --knexfile ./src/config/knexfile.ts migrate:make"
  },
  "keywords": [],
  "author": "Alex Mota, Jefferson Lima, Jorge Nobre, Wesley Teixeira",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "knex": "^2.3.0",
    "pg": "^8.8.0"
  }
}
